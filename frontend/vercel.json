{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps --no-audit --no-fund",
  "nodeVersion": "20.x"
}